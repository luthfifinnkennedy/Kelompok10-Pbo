from sqlalchemy import create_engine, Column, Integer, String, ForeignKey
from sqlalchemy.ext.declarative import declarative_base
from sqlalchemy.orm import sessionmaker, relationship

Base = declarative_base()

class Login(Base):

    __tablename__='Login'

    Username = Column(Integer, Primary_key=True)
    Password = Column(string)
        
class Admin(Base):

    __tablename__='Admin'
    id = Column(Integer, Primary_key=True)
    proposal = Column(String)
    StockBarang = Column(Integer)
    Pembelian = Column(String) 
         
class Penjual(Base):

    __tablename__='Penjual'
    id_client = Column(Integer, Primary_key=True)
    date = Column(Integer)
    transaksi = Column(Integer)
    
class Customer(Base):

    __tablename__='Penjual'
    id_user = Column(Integer, Primary_key=True)
    simpanan = Column(Integer)
    pinjaman = Column(Integer)
    angsuran = Column(Integer)
    denda = Column(Integer)

engine = craete_engine('sqlite:///:memory', echo=True)
Base.metadata.create_all(bind=engine)
session = sessionmaker(bind=engine)

session= Session()

session.close()


