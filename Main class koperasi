class login:
    def __init__(self, username, password):
        self.username = username
        self.password = password

    def login_autentikator(self):
        pass

class admin(Employee):
    def __init__(self, id_emp):
        self.id_emp = id_emp
    def proposal(self):
        self.proposal = proposal
    def stok_barang(self, kategori):
        self.barang = kategori
    def pembelian(self, id_barang):
        self.pembelian = id_barang
        
class penjual(Client):
    def __init__(self, id_client, date, transaksi):
        self.id_client = id_client
        self.date = date
        self.transaksi = transaksi
    def getTransaksi(self):
        return self.transaksi
    def setTransaksi(self, baru):
        self.transaksi = baru

class customer(User):
    def __init__(self, id_user, simpanan, pinjaman, angsuran, denda):
        self.id_user = id_user
        self.simpanan = simpanan
        self.pinjaman = pinjaman
        self.angsuran = angsuran
        self.denda = denda
    def getSimpanan(self):
        return self.simpanan
    def setSimpanan(self, baru):
        return self.simpanan = baru
    def getPinjaman(self):
        return self.pinjaman
    def setPinjaman(self, baru):
        return self.pinjaman = baru
    def getAngsuran(self):
        return self.angsuran
    def setAngsuran(self, baru):
        return self.angsuran = baru
    def getDenda(self):
        return self.denda
    def setDenda(self, baru):
        return self.denda = baru

