import Transaksi

class Denda():
    def __init__(self, idCustomer, tanggalPeminjaman, jumlahDenda):
        self.__idCustomer = idCustomer
        self.__tanggalPinjam = tanggalPeminjaman
        self.__jumlahDenda = jumlahDenda

    @property
    def idCustomer(self):
        return self.__idCustomer
    @idCustomer.setter
    def idCustomer(self, idCustomer):
        self.__idCustomer = idCustomer

    @property
    def tanggalPinjam(self):
        return self.__tanggalPinjam
    @tanggalPinjam.setter
    def tanggalPinjam(self, tanggalPinjam):
        self.__tanggalPinjam = tanggalPinjam

    @property
    def jumlahDenda(self):
        return self.__jumlahDenda
    @jumlahDenda.setter
    def jumlahDenda(self, jumlahDenda):
        self.__jumlahDenda = jumlahDenda

    '''
    Terimakasih untuk transaksi yang telah dilakukan.
    Silahkan transaksi ulang jika ada yang kurang atau salah

                                Have a Nice Day
    '''

    def cetakNota(self) -> bool:
        pass

